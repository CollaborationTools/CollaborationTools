    "eslint-plugin-vue": "^9.8.0",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "lint-staged": "^13.1.0",
    "nuxt": "3.0.0-rc.13",
    "peerjs": "^1.4.7",
    "pinia": "^2.0.27",
    "prettier": "^2.8.0",
    "simple-git-hooks": "^2.8.1",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "typescript": "^4.9.3",
