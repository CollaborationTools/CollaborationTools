{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node", "cypress-real-events"],
    "paths": {
      "cypress/*": [
        "../cypress/*"
      ],
      "@/*": [
        "../../platform/framework/*"
      ],
      "core/*": [
        "../../platform/core/*"
      ],
      "services/*": [
        "../../platform/services/*"
      ],
      "@@/*": [
        "../../*"
      ]
    }
  },
  "include": ["**/*.ts"]
}
