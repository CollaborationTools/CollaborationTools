{
  // https://v3.nuxtjs.org/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "noUncheckedIndexedAccess": true,
    "paths": {
      "@/*": [
        "./platform/framework/*"
      ],
      "components/*": [
        "./platform/framework/components/*"
      ],
      "composables/*": [
        "./platform/framework/composables/*"
      ],
      "core/*": [
        "./platform/core/*"
      ],
      "cypress/*": [
        "./test/cypress/*"
      ],
      "services/*": [
        "./platform/services/*"
      ],
      "stores/*": [
        "./platform/framework/stores/*"
      ],
      "tools/*": [
        "./tools/*"
      ],
      "@@/*": [
        "./*"
      ],
      "~/*": [
        "./.nuxt/*"
      ]
    }
  }
}
